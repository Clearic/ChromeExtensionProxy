{
    "name": "Proxy Extension",
    "description": "Quick proxy switcher",
    "version": "1.0.3",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "proxy"
    ],
    "incognito": "split",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "24": "icons/icon24.png",
            "32": "icons/icon32.png"
        }
    }
}
